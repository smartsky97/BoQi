$(function(){
	$("#index_content").tabs("add",{
		title:'波奇网后台管理',
		selected:true,
		closable:false,
		//href:"back/yc.html"
	});	
	$("#userInfo").tree({
		onClick:function(node){
			var obj = $("#index_content");
			if(node.id=="userinfo_manager"){
				if(obj.tabs("exists","用户信息管理")){
					obj.tabs("select","用户信息管理");
				}else{
					obj.tabs("add",{
						title:"用户信息管理",
						closable:true,
						fit:true,
						href:"back/UserInfo.jsp"
					});
				}
			}
		}
	});
	$("#goodsInfo").tree({
		onClick:function(node){
			var obj = $("#index_content");
			if(node.id=="brandinfo_manager"){
				if(obj.tabs("exists","品牌信息管理")){
					obj.tabs("select","品牌信息管理");
				}else{
					obj.tabs("add",{
						title:"品牌信息管理",
						closable:true,
						fit:true,
						href:"back/BrandInfo.jsp"
					});
				}
			}else if(node.id=="dogpro_manager"){
				if(obj.tabs("exists","狗狗商品信息管理")){
					obj.tabs("select","狗狗商品信息管理");
				}else{
					obj.tabs("add",{
						title:"狗狗商品信息管理",
						closable:true,
						fit:true,
						href:"back/dogproInfo.jsp"
					});
				}
			}
		}
	});
	$("#orderInfo").tree({
		onClick:function(node){
			var obj = $("#index_content");
			if(node.id=="ordercontent_manager"){
				if(obj.tabs("exists","订单信息")){
					obj.tabs("select","订单信息");
				}else{
					obj.tabs("add",{
						title:"订单信息",
						closable:true,
						fit:true,
						href:"back/OrderContent.jsp"
					});
				}
			}
		}
	});
});